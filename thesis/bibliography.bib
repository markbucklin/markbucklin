  author = {Herman, Max Charles and Cardoso, Mariana M B and Lima, Bruss and Sirotin, Yevgeniy B and Das, Aniruddha},
  file = {/files/reference/papers/zotero-attachments/Journal Article/herman_et_al.2017.simultaneously_estimating_the_task-related_and_stimulus-evoked_components_of3.pdf}
