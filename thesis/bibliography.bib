@article{lizier_jidt:_2014-2,
  title = {\{\vphantom\}{{JIDT}}\vphantom\{\}: \{\vphantom\}{{An}}\vphantom\{\} \{\vphantom\}{{Information}}\vphantom\{\}-\{\vphantom\}{{Theoretic}}\vphantom\{\} \{\vphantom\}{{Toolkit}}\vphantom\{\} for \{\vphantom\}{{Studying}}\vphantom\{\} the \{\vphantom\}{{Dynamics}}\vphantom\{\} of \{\vphantom\}{{Complex}}\vphantom\{\} \{\vphantom\}{{Systems}}\vphantom\{\}},
