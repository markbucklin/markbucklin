@article{herman_simultaneously_2017,
  title = {Simultaneously Estimating the Task-Related and Stimulus-Evoked Components of Hemodynamic Imaging Measurements},
  volume = {4},
  issn = {2329-423X, 2329-4248},
  url = {https://www.spiedigitallibrary.org/journals/Neurophotonics/volume-4/issue-3/031223/Simultaneously-estimating-the-task-related-and-stimulus-evoked-components-of/10.1117/1.NPh.4.3.031223.short},
  doi = {10.1117/1.NPh.4.3.031223},
  abstract = {Task-related hemodynamic responses contribute prominently to functional magnetic resonance imaging (fMRI) recordings. They reflect behaviorally important brain states, such as arousal and attention, and can dominate stimulus-evoked responses, yet they remain poorly understood. To help characterize these responses, we present a method for parametrically estimating both stimulus-evoked and task-related components of hemodynamic responses from subjects engaged in temporally predictable tasks. The stimulus-evoked component is modeled by convolving a hemodynamic response function (HRF) kernel with spiking. The task-related component is modeled by convolving a Fourier-series task-related function (TRF) kernel with task timing. We fit this model with simultaneous electrode recordings and intrinsic-signal optical imaging from the primary visual cortex of alert, task-engaged monkeys. With high strong$\backslash$textit\{R\}\$\^\{$\backslash$textrm\{2\}\}\$/strong, the model returns HRFs that are consistent across experiments and recording sites for a given animal and TRFs that entrain to task timing independent of stimulation or local spiking. When the task schedule conflicts with that of stimulation, the TRF remains locked to the task emphasizing its behavioral origins. The current approach is strikingly more robust to fluctuations than earlier ones and gives consistently, if modestly, better fits. This approach could help parse the distinct components of fMRI recordings made in the context of a task.},
  number = {3},
  journaltitle = {NPh NEUROW},
  date = {2017-07},
  pages = {31223},
  author = {Herman, Max Charles and Cardoso, Mariana M B and Lima, Bruss and Sirotin, Yevgeniy B and Das, Aniruddha},
  file = {/files/reference/papers/zotero-attachments/Journal Article/herman_et_al.2017.simultaneously_estimating_the_task-related_and_stimulus-evoked_components_of.pdf;/files/reference/papers/zotero-attachments/Journal Article/herman_et_al.2017.simultaneously_estimating_the_task-related_and_stimulus-evoked_components_of2.pdf}
}
