  date = {2017-09},
  keywords = {Computer Science - Information Theory,Mathematics - Optimization and Control},
  author = {Amari, Shun-ichi and Karakida, Ryo and Oizumi, Masafumi},
  file = {/files/reference/papers/zotero-attachments/Journal Article/amari_et_al.2017.information_ geometry _ connecting _ wasserstein _ distance _and.pdf}
}
