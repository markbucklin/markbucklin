@article{scholz_hemorrhage_nodate,
  title = {Hemorrhage Rates of External Ventricular Drain ({{EVD}}), Intracranial Pressure Gauge ({{ICP}}) or Combined {{EVD}} and {{ICP}} Gauge Placement within 48h of Endovascular Coil Embolization of Cerebral Aneurysms},
