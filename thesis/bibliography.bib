@article{jia_stimulating_2011,
  title = {Stimulating \{\vphantom\}{{Cardiac}}\vphantom\{\} \{\vphantom\}{{Muscle}}\vphantom\{\} by \{\vphantom\}{{Light}}\vphantom\{\}: \{\vphantom\}{{Cardiac}}\vphantom\{\} \{\vphantom\}{{Optogenetics}}\vphantom\{\} by \{\vphantom\}{{Cell}}\vphantom\{\} \{\vphantom\}{{Delivery}}\vphantom\{\}},
