@article{govorunova_new_2011,
  title = {New \{\vphantom\}{{Channelrhodopsin}}\vphantom\{\} with a \{\vphantom\}{{Red}}\vphantom\{\}-\{\vphantom\}{{Shifted}}\vphantom\{\} \{\vphantom\}{{Spectrum}}\vphantom\{\} and \{\vphantom\}{{Rapid}}\vphantom\{\} \{\vphantom\}{{Kinetics}}\vphantom\{\} from \{\vphantom\}{{Mesostigma}}\vphantom\{\} Viride},
  volume = {2},
