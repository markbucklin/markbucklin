  date = {2011-07},
  author = {Govorunova, Elena G and Spudich, Elena N and Lane, C Elizabeth and Sineshchekov, Oleg A and Spudich, John L},
  file = {/files/reference/papers/zotero-attachments/Journal Article/govorunova_et_al.2011.new_channelrhodopsin_with_a_red-shifted_spectrum_and_rapid_kinetics_from.pdf;/home/mark/Zotero/storage/2TRPJJQC/Govorunova et al. - 2011 - New Channelrhodopsin with a Red-Shifted Spectrum and Rapid Kinetics from Mesostigma viride.html}
