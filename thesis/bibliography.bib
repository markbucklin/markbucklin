  pages = {92},
  keywords = {microscopy,software,Humans,User-Computer Interface,Pathology,Predictive Value of Tests,Image Interpretation; Computer-Assisted},
  author = {Deroulers, Christophe and Ameisen, David and Badoual, Mathilde and Gerin, Chlo\'e and Granier, Alexandre and Lartaud, Marc},
  eprinttype = {pmid},
