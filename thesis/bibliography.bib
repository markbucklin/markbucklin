  author = {Cilibrasi, Rudi and Vitanyi, Paul},
  file = {/home/mark/Zotero/storage/A62KVC49/Cilibrasi and Vitanyi - 2003 - Clustering by compression.pdf;/home/mark/Zotero/storage/HANKHQEB/0312044.html}
