  author = {Pnevmatikakis, Eftychios A. and Giovannucci, Andrea},
  file = {/home/mark/Zotero/storage/EEJ6NMP7/Pnevmatikakis and Giovannucci - 2017 - NoRMCorre An online algorithm for piecewise rigid.pdf;/home/mark/Zotero/storage/R6LPE8U7/S0165027017302753.html}
}

@article{aghayee_particle_2017,
