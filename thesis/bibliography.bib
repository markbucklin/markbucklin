  keywords = {parameter optimization,likelihood,Model selection,probability density function,quantitative modeling},
  author = {Kimura, Akatsuki and Celani, Antonio and Nagao, Hiromichi and Stasevich, Timothy and Nakamura, Kazuyuki}
