@article{speier_evaluating_2013,
  title = {Evaluating \{\vphantom\}{{True}}\vphantom\{\} \{\vphantom\}{{BCI}}\vphantom\{\} \{\vphantom\}{{Communication}}\vphantom\{\} \{\vphantom\}{{Rate}}\vphantom\{\} through \{\vphantom\}{{Mutual}}\vphantom\{\} \{\vphantom\}{{Information}}\vphantom\{\} and \{\vphantom\}{{Language}}\vphantom\{\} \{\vphantom\}{{Models}}\vphantom\{\}},
