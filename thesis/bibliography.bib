  keywords = {Relative entropy,Adjusted distance,Distance metric,Generalized relative entropy,Upper bound},
  author = {Liu, Shuai and Lu, Mengye and Liu, Gaocheng and Pan, Zheng},
  file = {/files/reference/papers/zotero-attachments/Journal Article/liu_et_al.2017.a_novel_distance_metric.pdf},
