  date = {2008-03-13},
  pages = {661-672},
  keywords = {SYSNEURO,SYSBIO},
  author = {Holekamp, Terrence F. and Turaga, Diwakar and Holy, Timothy E.},
  file = {/home/mark/Zotero/storage/VE3QMTPH/Holekamp et al. - 2008 - Fast Three-Dimensional Fluorescence Imaging of Act.pdf;/home/mark/Zotero/storage/52SF9QBD/S0896627308000445.html}
}
