@article{mayberg_deep_2005,
  title = {Deep Brain Stimulation for Treatment-Resistant Depression},
  volume = {45},
  issn = {0896-6273},
  url = {http://www.sciencedirect.com/science/article/pii/S089662730500156X\%0020http://linkinghub.elsevier.com/retrieve/pii/S089662730500156X},
  doi = {10.1016/j.neuron.2005.02.014},
  abstract = {Summary Treatment-resistant depression is a severely disabling disorder with no proven treatment options once multiple medications, psychotherapy, and electroconvulsive therapy have failed. Based on our preliminary observation that the subgenual cingulate region (Brodmann area 25) is metabolically overactive in treatment-resistant depression, we studied whether the application of chronic deep brain stimulation to modulate BA25 could reduce this elevated activity and produce clinical benefit in six patients with refractory depression. Chronic stimulation of white matter tracts adjacent to the subgenual cingulate gyrus was associated with a striking and sustained remission of depression in four of six patients. Antidepressant effects were associated with a marked reduction in local cerebral blood flow as well as changes in downstream limbic and cortical sites, measured using positron emission tomography. These results suggest that disrupting focal pathological activity in limbic-cortical circuits using electrical stimulation of the subgenual cingulate white matter can effectively reverse symptoms in otherwise treatment-resistant depression.},
  number = {5},
  journaltitle = {Neuron},
  date = {2005-03},
  pages = {651-660},
  author = {Mayberg, Helen S. and Lozano, Andres M. and Voon, Valerie and McNeely, Heather E. and Seminowicz, David and Hamani, Clement and Schwalb, Jason M. and Kennedy, Sidney H.},
  file = {/files/reference/papers/zotero-attachments/Journal Article/mayberg_et_al.2005.deep_brain_stimulation_for_treatment-resistant_depression.pdf;/files/reference/papers/zotero-attachments/Journal Article/mayberg_et_al.2005.deep_brain_stimulation_for_treatment-resistant_depression2.pdf;/home/mark/Zotero/storage/6P4LT4QI/Mayberg et al. - 2005 - Deep brain stimulation for treatment-resistant depression(2).html;/home/mark/Zotero/storage/7Z2HJ4VM/Mayberg et al. - 2005 - Deep brain stimulation for treatment-resistant depression.html}
